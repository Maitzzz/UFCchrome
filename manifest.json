{
  "name": "UFC",
  "description": "UFC",
  "version": "0.1",
  "permissions": ["gcm", "storage", "notifications"],
  "icons": { "128": "images/ic_launcher.png" },
  "browser_action": {
    "default_title": "UFC",
    "default_icon": "images/ic_launcher.png",
    "default_popup":"popup.html"
  },
  "manifest_version": 2,
  "background":
  {
    "scripts": ["js/jquery.min.js","js/page.js"]
  },

  "content_scripts": [{
    "js": [
      "js/page.js"
    ],
    "css": [ "css/empty.css" ],
    "run_at": "document_start",
    "matches": [ "<all_urls>" ]

  }]
}

